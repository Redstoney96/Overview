<Window x:Class="Overview.SettingsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Overview"
        mc:Ignorable="d"
        Title="Settings" Height="315" Width="540" Background="Gray">
    
    <WrapPanel x:Name="mainPanel" Margin="10" HorizontalAlignment="Center">
        <Label Content="General" FontWeight="Bold" Background="#FFFFAF00" Width="540"/>
        <Button Click="autoStartOn" Margin="5" Content="Enable automatic Start" Background="#FFBDBDBD" BorderBrush="#FF3E3E3E" BorderThickness="3,1,3,3" Height="35"/>
        <Button Click="autoStartOff" Margin="5" Content="Disable automatic Start" Background="#FFBDBDBD" BorderBrush="#FF3E3E3E" BorderThickness="3,1,3,3" Height="35"/>
        <Label Content="Config" FontWeight="Bold" Background="#FFFFAF00" Width="540"/>
    </WrapPanel>
</Window>
